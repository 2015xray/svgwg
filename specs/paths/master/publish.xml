<!-- Publication script for SVG Paths. -->

<publish-conf xmlns='http://mcc.id.au/ns/local'>
  <title>SVG Paths</title>
  <short-title>SVG Paths</short-title>
  <maturity>ED</maturity>
  <output use-publish-directory='true' publish-directory='build/publish'/>

  <!--
    Uncomment the publication date when it is time to publish on
    http://www.w3.org/TR/.  While an Editor's Draft, leave this
    commented out and the last commit date will be used as the
    document's date.
    -->
  
  <!--
  <publication-date>2015-04-09</publication-date>
  -->

  <versions>
    <cvs href='https://svgwg.org/specs/paths/'/>
    <!--
    <this href='http://www.w3.org/TR/2015/WD-svg-paths-2015xxxx/'/>
    <previous href='http://www.w3.org/TR/2014/WD-svg-strokes-2014xxxx/'/>
    <latest href='http://www.w3.org/TR/svg-paths/'/>
    -->
  </versions>

  <definitions href='definitions.xml' specid='Paths'/>
  <definitions href='../../../master/definitions.xml' base='https://svgwg.org/svg2-draft/' specid='SVG2'/>
  <definitions href='../../../master/definitions-masking.xml' base='http://www.w3.org/TR/2014/CR-css-masking-1-20140826/' specid='Masking' mainspec='SVG2'/>

  <resource href='style'/>
  <resource href='images'/>

  <index name='Overview'/>
</publish-conf>
